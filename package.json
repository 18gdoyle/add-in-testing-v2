    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/express": "^4.17.14",
    "@types/jquery": "^3.5.14",
    "@types/node": "^16.11.41",
    "@types/office-js": "^1.0.266",
    "@types/office-runtime": "^1.0.24",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
